{
  "expo": {
    "name": "BK Map",
    "slug": "bkmap",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/bkmap.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/90e2bdfa-33b1-48f1-b760-c6e8c9365cac"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hpcc.expo-redux-template",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Samsung A6 to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hpcc.expo_redux_template",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow Samsung A6 to access camera."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "54f9db62-2cf7-44be-9c39-89ef71cf2875"
      }
    },
    "owner": "duy.pham20092001"
  }
}
